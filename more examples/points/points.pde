void setup() {

size(500, 500);
noLoop();
}

void draw() {
smooth();
background(100);
stroke(255); // color
strokeWeight(10);//size of the point
point(200, 200);
strokeWeight(5);
stroke(12);
point(200, 200);

stroke(255);
strokeWeight(30);
point(300, 200);
strokeWeight(50);
point(200, 300);
strokeWeight(80);
point(300, 300);
}
